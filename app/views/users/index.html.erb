<%= form_for @user do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>

  <%= form.label :email %>
  <%= form.text_field :email %>

  <%= form.label :password %>
  <%= form.password_field :password %>

  <%= form.label :confirm_password %>
  <%= form.password_field :confirm_password %>


  <%= form.submit "Submit" %>

  
<% end %>
